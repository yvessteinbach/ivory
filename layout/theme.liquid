            {% if customer %}
              {% if customer.first_name != blank %}
                {% capture first_name %}<a class="header__login" href="/account">{{ customer.first_name }}</a>{% endcapture %}
                <a class="header__login" href="/account">{{first_name }}</a>
              {% else %}
                <a href="/account">{{ 'layout.customer.account' | t }}</a>
              {% endif %}
              <a href="/account/logout">{{ 'layout.customer.log_out' | t | customer_logout_link }}</a>
            {% else %}
              <a class="header__login" href="/account/login">{{ 'layout.customer.log_in' | t | customer_login_link }}</a>
            {% endif %}
  
            <a class="header__cart-container" href="/cart">Cart | <span data-cart-number="" class="header__cart--number">{{ cart.item_count }}</span></a>
  
